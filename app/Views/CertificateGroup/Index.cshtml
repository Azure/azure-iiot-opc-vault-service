@model IEnumerable<Microsoft.Azure.IIoT.OpcUa.Services.GdsVault.Api.Models.CertificateGroupConfigurationApiModel>

@{
    ViewBag.Title = "CertificateGroups";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>List of Certificate Groups</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CertificateType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubjectName)
        </th>
        <th></th>
    </tr>

    @foreach (var group in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(group.Name, "Details", new { id = group.Name })
            </td>
            <td>
                @Html.DisplayFor(groupItem => group.CertificateType)
            </td>
            <td>
                @Html.DisplayFor(groupItem => group.SubjectName)
            </td>
        </tr>
    }

</table>

<p>
</p>
