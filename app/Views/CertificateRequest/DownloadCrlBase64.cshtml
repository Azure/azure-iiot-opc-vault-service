@model Microsoft.Azure.IIoT.OpcUa.Services.Vault.App.Models.CrlDetailsApiModel

<h2>View CRL Details</h2>

<div>
    <hr />
    <dl class="dl-horizontal">

        <dt>
            @Html.DisplayNameFor(model => model.Issuer)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Issuer)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.UpdateTime)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UpdateTime)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.NextUpdateTime)
